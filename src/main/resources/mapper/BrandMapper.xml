<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itheima.mapper.BrandMapper">

    <resultMap id="BaseResultMap" type="brand">
            <id property="brandId" column="brand_id" jdbcType="INTEGER"/>
            <result property="brandName" column="brand_name" jdbcType="VARCHAR"/>
            <result property="brandLeter" column="brand_leter" jdbcType="CHAR"/>
            <result property="brandDesc" column="brand_desc" jdbcType="VARCHAR"/>
    </resultMap>
    <select id="findAllBrand" resultType="com.itheima.entity.Brand">

SELECT * FROM brand


    </select>


</mapper>
